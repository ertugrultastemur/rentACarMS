package com.example.carservice.dto;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
public class CarIdDto {

	private int id;
	
	private int state;
	
	public CarIdDto convert(int id, int state) {
		return new CarIdDto(id,state);
	}

}
